<table id="minion_wave">
  <colgroup>
    <col style="width: 33%">
    <col style="width: 33%">
    <col style="width: 33%">
  </colgroup>
  <tr>
    <td colspan="3">
      <div id="aa">
      </div>

    </td>
  </tr>
  <tr>
    <td>
      <div id="0_minion_wrapper" class="minion_wrapper">
        <div id="dmg"></div>
        <div id="hp"></div>
        <img id="minion_src"></img>
        <div id="0_focus" class="focus">
          0
        </div>
      </div>

    </td>
