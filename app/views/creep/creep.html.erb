      <div id="item_display">
        <div class="left" style="padding-top:10px"><b>Gold:</b> $<span id="shop_gold">0</span></div>
        <table id="item_display_table">
          <tr>
            <td rowspan="2">
              <div style="width:150px;height:150px;">
                <img id="item_display_img" class="shop_img"></img>
              </div>
            </td>
            <td colspan="2">
              <span id="item_display_title"></span>
            </td>
          </tr>
          <tr>
            <td style="width:150px">
              <span id="item_display_description"></span>
            </td>
            <td>
              <input type="button" class="buybtn" onclick="shop.BuyItem();" id="item_display_button" value="Buy"/>
              <input type="button" onclick="game.CloseShop();" id="item_display_button" value="Close"/>
            </td>
          </tr>
        </table>
      </div>
    </td>
  </tr>
  <tr>
    <td colspan="3" id="dependent_items">

